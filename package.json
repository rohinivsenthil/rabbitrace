{
  "name": "rabbitrace",
  "displayName": "Rabbitrace",
  "description": "A developer/management tool to help monitor RabbitMQ queues and exchanges",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "rabbitmq",
    "amqp",
    "management",
    "messages",
    "queues",
    "publish"
  ],
  "activationEvents": [
    "onView:rabbitmq.connections",
    "onCustomEditor:rabbitmq.exchange",
    "onCustomEditor:rabbitmq.queue",
    "onCommand:rabbitmq.newConnection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "rabbitmq.exchange",
        "displayName": "Rabbitrace Exchange",
        "selector": [
          {
            "filenamePattern": "rabbitmq-exchange:**"
          }
        ]
      },
      {
        "viewType": "rabbitmq.queue",
        "displayName": "Rabbitrace Queue",
        "selector": [
          {
            "filenamePattern": "rabbitmq-queue:**"
          }
        ]
      }
    ],
    "views": {
      "rabbitmq": [
        {
          "id": "rabbitmq.connections",
          "name": "Connections"
        },
        {
          "id": "rabbitmq.exchanges",
          "name": "Exchanges"
        },
        {
          "id": "rabbitmq.queues",
          "name": "Queues"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rabbitmq",
          "title": "Rabbitrace",
          "icon": "media/icon.png"
        }
      ]
    },
    "commands": [
      {
        "command": "rabbitmq.connections.new",
        "title": "New Connection",
        "icon": "$(plus)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.connections.edit",
        "title": "Edit Connection",
        "icon": "$(edit)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.connections.remove",
        "title": "Remove Connection",
        "icon": "$(trash)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.connections.connect",
        "title": "Connect",
        "icon": "$(debug-start)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.connections.disconnect",
        "title": "Disconnect",
        "icon": "$(debug-stop)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.connections.refresh",
        "title": "Refresh",
        "icon": "$(refresh)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.exchanges.new",
        "title": "New Exchange",
        "icon": "$(plus)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.exchanges.delete",
        "title": "Delete",
        "icon": "$(trash)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.exchanges.refresh",
        "title": "Refresh",
        "icon": "$(refresh)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.queues.new",
        "title": "New Queue",
        "icon": "$(plus)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.queues.delete",
        "title": "Delete",
        "icon": "$(trash)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.queues.details",
        "title": "Details",
        "icon": "$(debug-disconnect)",
        "category": "Rabbitrace"
      },
      {
        "command": "rabbitmq.queues.refresh",
        "title": "Refresh",
        "icon": "$(refresh)",
        "category": "Rabbitrace"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "rabbitmq.connections.new",
          "when": "view == rabbitmq.connections",
          "group": "navigation"
        },
        {
          "command": "rabbitmq.exchanges.new",
          "when": "view == rabbitmq.exchanges",
          "group": "navigation"
        },
        {
          "command": "rabbitmq.exchanges.refresh",
          "when": "view == rabbitmq.exchanges",
          "group": "navigation"
        },
        {
          "command": "rabbitmq.queues.new",
          "when": "view == rabbitmq.queues",
          "group": "navigation"
        },
        {
          "command": "rabbitmq.queues.refresh",
          "when": "view == rabbitmq.queues",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "rabbitmq.connections.connect",
          "when": "view == rabbitmq.connections && viewItem == not-connected",
          "group": "inline"
        },
        {
          "command": "rabbitmq.connections.disconnect",
          "when": "view == rabbitmq.connections && viewItem == connected",
          "group": "inline"
        },
        {
          "command": "rabbitmq.connections.edit",
          "when": "view == rabbitmq.connections",
          "group": "inline"
        },
        {
          "command": "rabbitmq.connections.remove",
          "when": "view == rabbitmq.connections",
          "group": "inline"
        },
        {
          "command": "rabbitmq.exchanges.delete",
          "when": "view == rabbitmq.exchanges && viewItem != error",
          "group": "inline"
        },
        {
          "command": "rabbitmq.queues.delete",
          "when": "view == rabbitmq.queues && viewItem != error",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts && prettier -c .",
    "lint:fix": "eslint src --fix --ext ts; prettier -w .",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mocha": "^9.2.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^3.1.3",
    "postcss-loader": "^6.2.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-loader": "^3.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.29",
    "axios": "^0.26.1"
  }
}
